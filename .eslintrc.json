{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/recommended"
      ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "parser": "babel-eslint"
    },
    "rules": {
        "vue/max-attributes-per-line": "off",
        "prefer-const": "error",
        "no-console": "warn",
        "indent": "off",
        "quotes": "off",
        "eol-last": "off",
        "space-before-function-paren": "off",
        "comma-spacing": "off",
        "arrow-spacing": "off",
        "no-trailing-spaces": "off",
        "semi": "off",
        "space-infix-ops": "off",
        "keyword-spacing": "off",
        "space-in-parens": "off",
        "object-curly-spacing": "off",
        "func-call-spacing": "off",
        "dot-location": "off"
    }
}